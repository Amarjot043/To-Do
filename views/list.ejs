<%- include("header") -%>

<body>

    <div class="box" id="heading">
        <h1><%= listTitle %></h1>
    </div>

    
    <div class="box">
        <form action=<%= action %> method="POST">
            <input type="text" name="todo" placeholder="Enter task" autocomplete="off">
            <button type="submit" name="button">+</button></form>
        <%  for(var i = 0;i<newListItems.length;i++){ %>
            <div class="item">
                <input type="checkbox"> <p><%=newListItems[i]%></p></div>
        <%}%>
    </div>

        
    
             
<%- include("footer") -%>

    
